echo 'To embed the models please refer the link below each model<br>'>index.html
for n in $(ls ../render/); do cp -f Render.html $n.html; sed -i "s/car_model.stl/${n}/g" $n.html; echo '<iframe src="https://ayeinterns.github.io/ayeinterns-winter2021/sahaja/3js/'${n}'.html" width=800 height=600>iframe not supported</iframe>' >$n.embed; cat $n.embed >> index.html; echo '<br><a href="https://ayeinterns.github.io/ayeinterns-winter2021/sahaja/3js/'${n}'.embed">'Embed $n'</a>'>>index.html; echo '<br><a href="https://ayeinterns.github.io/ayeinterns-winter2021/sahaja/3js/'${n}'.html">'View $n'</a><br>'>>index.html; done
mv index.html view_all.html
cat view_all.html | grep -v iframe >index.html
